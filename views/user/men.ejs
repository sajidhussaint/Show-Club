<%- include('./header') %>
		<div class="breadcrumbs">
			<div class="container">
				<div class="row">
					<div class="col">
						<p class="bread"><span><a href="/">Home</a></span> / <span>Men</span></p>
					</div>
				</div>
			</div>
		</div>

		<div class="breadcrumbs-two">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="breadcrumbs-img" style="background-image: url(images/main/cover-img-1.jpg);">
							<h2 style="color: white;">Men's</h2>
						</div>
						<div class="menu text-center">
							<p><a href="/search?searchItem=">New Arrivals</a> <a href="/search?searchItem=">Best Sellers</a><a href="/search?searchItem=">Sale</a></p>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="colorlib-featured">
			<div class="container">
				<div class="row">
					<% categories.forEach(function(category,index) { %>
						<div class="col-sm-4 text-center">
							<div class="featured">
								<div class="featured-img featured-img-2" style="background-image: url(images/main/imgr-<%=index+1%>.jpg);">
									<h2><%=category.name%>
									<p><a href="/search?category=<%=category._id %>" class="btn btn-primary btn-lg">Shop now</a></p>
								</div>
							</div>
						</div>
						<% }); %>  
	
				</div>
			</div>
		</div>

		<div class="colorlib-product">
			<div class="container">
				<div class="row">
					<div class="col-sm-8 offset-sm-2 text-center colorlib-heading colorlib-heading-sm">
						<h2>View All Products</h2>
					</div>
				</div>
				<div class="row row-pb-md">
					<% product.forEach(function(product,index) { %>
						<div class="col-lg-3 mb-4 text-center main-cont">
						  <div class="product-entry border">
							<a href="/product-detail?productid=<%=product._id %>" class="prod-img">
							  <img src="/admin/cropped/<%= product.image[0] %>"
								class="img-fluid"
								alt="Free html5 bootstrap 4 template"
							  />
							</a>
							<div class="desc">
							  <h2><a href="#"><%=product.name %></a></h2>
							  <span class="price">â‚¹<%=product.price %></span>
							</div>
						  </div>
						</div>
						<% }); %>
				</div>
				<!-- <div class="row">
					<div class="col-md-12 text-center">
						<div class="block-27">
		               <ul>
			               <li><a href="#"><i class="ion-ios-arrow-back"></i></a></li>
		                  <li class="active"><span>1</span></li>
		                  <li><a href="#">2</a></li>
		                  <li><a href="#">3</a></li>
		                  <li><a href="#">4</a></li>
		                  <li><a href="#">5</a></li>
		                  <li><a href="#"><i class="ion-ios-arrow-forward"></i></a></li>
		               </ul>
		            </div>
					</div>
				</div> -->
			</div>
		</div>

		<div class="colorlib-partner">
			<div class="container">
				<div class="row">
					<div class="col-sm-8 offset-sm-2 text-center colorlib-heading colorlib-heading-sm">
						<h2>Trusted Partners</h2>
					</div>
				</div>
				<div class="row">
					<div class="col partner-col text-center">
						<img src="images/main/brand-1.jpg" class="img-fluid" alt="Free html4 bootstrap 4 template">
					</div>
					<div class="col partner-col text-center">
						<img src="images/main/brand-2.jpg" class="img-fluid" alt="Free html4 bootstrap 4 template">
					</div>
					<div class="col partner-col text-center">
						<img src="images/main/brand-3.jpg" class="img-fluid" alt="Free html4 bootstrap 4 template">
					</div>
					<div class="col partner-col text-center">
						<img src="images/main/brand-4.jpg" class="img-fluid" alt="Free html4 bootstrap 4 template">
					</div>
					<div class="col partner-col text-center">
						<img src="images/main/brand-5.jpg" class="img-fluid" alt="Free html4 bootstrap 4 template">
					</div>
				</div>
			</div>
		</div>
		<%- include('./footer') %>

